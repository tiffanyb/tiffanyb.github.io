title = {Machine-Code Verification for Multiple Architectures: An Application of Decompilation into Logic},
url = {http://ieeexplore.ieee.org/document/4689183/},
year = {2008}
}
@inproceedings{Myreen2012,
abstract = {This paper presents improvements to a technique which aids veriﬁcation of machine-code programs. This technique, called decompilation into logic, allows the veriﬁer to only deal with tractable extracted models of the machine code rather than the concrete code itself. Our improvements make decompilation simpler, faster and more generally applicable. In particular, the new technique allows the veriﬁer to avoid tedious reasoning directly in the underlying machine-code Hoare logic or the model of the instruction set architecture. The method described in this paper has been implemented in the HOL4 theorem prover.},
author = {Myreen, Mangus O. and Gordon, Michael J. C. and Slind, Konrad},
booktitle = {Proceedings of the 12th Conference on Formal Methods in Computer-Aided Design},
