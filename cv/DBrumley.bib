@article{Cesare2013,
abstract = {Developers sometimes maintain an internal copy of another software or\nfork development of an existing project. This practice can lead to\nsoftware vulnerabilities when the embedded code is not kept up to date\nwith upstream sources. We propose an automated solution to identify\nclones of packages without any prior knowledge of these relationships.\nWe then correlate clones with vulnerability information to identify\noutstanding security problems. This approach motivates software\nmaintainers to avoid using cloned packages and link against system wide\nlibraries. We propose over 30 novel features that enable us to use to\nuse pattern classification to accurately identify package-level clones.\nTo our knowledge, we are the first to consider clone detection as a\nclassification problem. Our results show our system, Clonewise, compares\nwell to manually tracked databases. Based on our work, over 30 unknown\npackage clones and vulnerabilities have been identified and patched.},
author = {Cesare, Silvio and Xiang, Yang and Zhang, Jun},
doi = {10.1007/978-3-319-04283-1_13},
isbn = {978-3-319-04283-1; 978-3-319-04282-4},
issn = {1867-8211},
journal = {Security and Privacy in Communication Networks, Securecomm 2013},
keywords = {Linux,Vulnerability detection,code clone},
pages = {197--215},
title = {Clonewise - Detecting Package-Level Clones Using Machine Learning},
volume = {127},
year = {2013}
}
