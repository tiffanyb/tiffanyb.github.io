@inproceedings{Yu2008,
abstract = {We present an automata-based approach for the verification of string\noperations in PHP programs based on symbolic string analysis. String\nanalysis is a static analysis technique that determines the values\nthat a string expression can take during program execution at a given\nprogram point. This information can be used to verify that string\nvalues are sanitized properly and to detect programming errors and\nsecurity vulnerabilities. In our string analysis approach, we encode\nthe set of string values that string variables can take as automata.\nWe implement all string functions using a symbolic automata representation\n(MBDD representation from the MONA automata package) and leverage\nefficient manipulations on MBDDs, e.g., determinization and minimization.\nParticularly, we propose a novel algorithm for language-based replacement.\nOur replacement function takes three DFAs as arguments and outputs\na DFA. Finally, we apply a widening operator defined on automata\nto approximate fixpoint computations. If this conservative approximation\ndoes not include any bad patterns (specified as regular expressions),\nwe conclude that the program does not contain any errors or vulnerabilities.\nOur experimental results demonstrate that our approach works quite\nwell in checking the correctness of sanitization operations in real-world\nPHP applications.},
author = {Yu, Fang and Bultan, Tevfik and Cova, Marco and Ibarra, Oscar H.},
booktitle = {Proceedings of the 15th International SPIN Workshop},
doi = {10.1007/978-3-540-85114-1_21},
isbn = {3540851135},
issn = {03029743},
pages = {306--324},
publisher = {Springer},
title = {Symbolic String Verification: An Automata-Based Approach},
url = {http://link.springer.com/10.1007/978-3-540-85114-1_21},
year = {2008}
}
@inproceedings{Yu2009,
abstract = {Given a program and an attack pattern (specified as a regular expression), we automatically generate string-based vulnerability signatures, i.e., a characterization that includes all malicious inputs that can be used to generate attacks. We use an automata-based string analysis framework. Using forward reachability analysis we compute an over-approximation of all possible values that string variables can take at each program point. Intersecting these with the attack pattern yields the potential attack strings if the program is vulnerable. Using backward analysis we compute an over-approximation of all possible inputs that can generate those attack strings. In addition to identifying existing vulnerabilities and their causes, these vulnerability signatures can be used to filter out malicious inputs. Our approach extends the prior work on automata-based string analysis by providing a backward symbolic analysis that includes a symbolic pre-image computation for deterministic finite automata on common string manipulating functions such as concatenation and replacement.},
author = {Yu, Fang and Alkhalaf, Muath and Bultan, Tevfik},
booktitle = {Proceedings of the 2009 IEEE/ACM International Conference on Automated Software Engineering},
doi = {10.1109/ASE.2009.20},
isbn = {978-1-4244-5259-0},
issn = {1527-1366},
pages = {605--609},
publisher = {IEEE},
title = {Generating Vulnerability Signatures for String Manipulating Programs Using Automata-Based Forward and Backward Symbolic Analyses},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5431724},
year = {2009}
}
