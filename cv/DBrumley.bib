@inproceedings{Sahai2014,
abstract = {We introduce a new technique, that we call punctured programs, to apply indistinguishability obfuscation towards cryptographic problems. We use this technique to carry out a systematic study of the applicability of indistinguishability obfuscation to a variety of cryptographic goals. Along the way, we resolve the 16-year-old open question of Deniable Encryption, posed by Canetti, Dwork, Naor, and Ostrovsky in 1997: In deniable encryption, a sender who is forced to reveal to an adversary both her message and the randomness she used for encrypting it should be able to convincingly provide "fake" randomness that can explain any alternative message that she would like to pretend that she sent. We resolve this question by giving the first construction of deniable encryption that does not require any pre-planning by the party that must later issue denial. In addition, we show the generality of our punctured programs technique by also construction variety of core cryptographic objects from indistinguishability obfuscation and one-way functions (or close variants.) In particular we obtain: public key encryption, short "hash-and-sign" selective secure signatures, chosen-ciphertext secure public key encryption, non-interactive zero knowledge proofs (NIZKs), injective trapdor functionos, and oblivious transfer. There results suggest the possibility of indistinguishability obfuscation becoming a "central hub" for cryptography.},
author = {Sahai, Amit and Waters, Brent},
booktitle = {Proceedings of the 46th Annual ACM Symposium on Theory of Computing},
doi = {10.1145/2591796.2591825},
