@techreport{Dhem1998,
abstract = {When the running time of a cryptographic algorithm is nonconstant, timing measurements can leak informations about the secret key. This idea, rst publicly introduced by Kocher, is developed here to attack an earlier version of the CASCADE smart cardy. We propose several improvements on Kocher's ideas, leading to a practical implementation that is able to break a 512-bit key in a few minutes, provided we are able to collect 300000 timing measurements (128-bit keys can be recovered in a few seconds using a personal computer and less than 10000 samples). We therefore show that the timing attack represents an important threat against cryptosystems, whichmust be very seriously taken into account.},
author = {Dhem, J and Koeune, F and Leroux, P and Mestre, P and Quisquater, J and Willems, J},
